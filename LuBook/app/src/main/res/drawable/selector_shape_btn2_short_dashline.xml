<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_window_focused="false">
        <shape android:shape="rectangle">
            <!--圆角-->
            <corners android:radius="2dp"/>
            <!--填充色-->
            <solid android:color="@color/btn_blue"/>
            <!--描边，设置描边为虚线，其中android:dashWidth表示'-'这样一个横线的宽度，android:dashGap表示之间隔开的距离-->
            <stroke android:width="12dp" android:color="@color/colorAccent" android:dashGap="1dp" android:dashWidth="2dp"/>
        </shape>

    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <!--圆角-->
            <corners android:radius="2dp"/>
            <!--填充色-->
            <solid android:color="@color/colormusicBar"/>
            <!--描边，设置描边为虚线，其中android:dashWidth表示'-'这样一个横线的宽度，android:dashGap表示之间隔开的距离-->
            <stroke android:width="12dp" android:color="@color/colorAccent" android:dashGap="1dp" android:dashWidth="2dp"/>
        </shape>
    </item>
</selector>


